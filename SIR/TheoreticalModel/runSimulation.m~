clear all; close all; clc;
tend  = 200;
dt    = 0.1;
ic    = [0.9995,1-0.9995,0];

%% === SIRDE(tend, dt, ic)=======================
SIR1 = SIRDE(tend,0.2,ic, 2000);

SIR1.beta = .1;
SIR1.gamma = .006;

% ===== V18 =======
SIR1.beta = .16;
SIR1.gamma = .008;

% ===== V17 =======
SIR1.beta = 0.4;
SIR1.gamma = 0.0;

% ====== V13 parameters ===========
%SIR1.beta = .11;
%SIR1.gamma = .013;

% ====== V7 parameters ===========
%SIR1.beta = .16;
%SIR1.gamma = .008;
%SIR1.mu    = 0.3;


SIR1.Simulate(0);
%SIR1.plot('LegendOn')
%grid on

SIR1.getEigen();
SIR1.singlePlot('All',1)


open('../SIRXCode/SIR/Results/res_3-2-2015/sim_v18.fig')
set(gcf,'units','normalized','outerposition',[0 0 1 1])
hold on

SIR1.plot('LegendOn')
